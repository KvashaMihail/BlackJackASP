@{
    ViewBag.Title = "Player Menu";
    Layout = "_Layout";
}
<div>
    <form method="get" asp-controller="Home" asp-action="Register">
        <input type="submit" style="width: 250px" class="btn btn-outline-primary my-control button-my-outline" value="Register" />
    </form>
    <form method="get" asp-controller="Home" asp-action="Login">
        <input type="submit" style="width: 250px" class="btn btn-outline-success my-control button-my-outline" value="Login" />
    </form>
</div>
